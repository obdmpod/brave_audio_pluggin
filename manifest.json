{
    "manifest_version": 3,
    "name": "Audio Limiter for Brave",
    "version": "1.0",
    "description": "Limits audio output to a set decibel threshold with output control.",
    "permissions": ["tabs", "storage"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {

      }
    }
  }
  